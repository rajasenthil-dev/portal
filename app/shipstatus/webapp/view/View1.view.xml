<mvc:View controllerName="shipstatus.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:l="sap.ui.layout"
    xmlns:t="sap.ui.table"
    xmlns:sf="sap.ui.comp.smartfilterbar"
    xmlns:st="sap.ui.comp.smarttable"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    displayBlock="true"
    xmlns:core="sap.ui.core"
	>
    <f:DynamicPage
            id="dynamicPage"
            showFooter="true"
        >
            <f:header>
                <f:DynamicPageHeader
                    id="_IDGenDynamicPageHeader1"
                    class="noPadding"
                >
                    <FlexBox
                        id="_IDGenFlexBox6"
                        width="100%"
                        direction="Row"
                        wrap="Wrap"
                        justifyContent="SpaceBetween"
                        class="headerPadding sapUiMediumMarginBottom borderBottom"
                    >
                        <HeaderContainer
                            id="container1"
                            scrollStep="200"
                            height="100px"
                        >
                            <FlexBox
                                id="_IDGenFlexBox7"
                                alignItems="Center"
                                height="100px"
                            >
                                <FlexBox
                                    id="_IDGenFlexBox8"
                                    alignItems="Center"
                                    height="75px"
                                    class="imgBG"
                                >
                                    <Image
                                        id="logoImage"
                                        src="/img/logoipsum-325.svg"
                                        width="125px"
                                        decorative="false"
                                        press="onLogoPressed"
                                        tooltip="SAP Logo"
                                    >
                                        <layoutData>
                                            <OverflowToolbarLayoutData
                                                id="_IDGenOverflowToolbarLayoutData1"
                                                priority="NeverOverflow"
                                            />
                                        </layoutData>
                                    </Image>
                                </FlexBox>
                            </FlexBox>
                        </HeaderContainer>

                        
                    </FlexBox>

                    <sf:SmartFilterBar
                        id="bar0"
                        entitySet="SHIPPINGSTATUS"
                        useDateRangeType="true"
                        persistencyKey="SmartFilterBar_SHIPPINGSTATUS"
                        smartVariant="pageVariant"
                        liveMode="true"
                        search="onSearch"
                    >
                        <sf:controlConfiguration>
                            <sf:ControlConfiguration id="_IDGenControlConfiguration1"
                                key="AUDAT"
                                visibleInAdvancedArea="true"
                                conditionType="{
                                    module: 'sap.ui.comp.config.condition.DateRangeType'
                                }" />
                            <sf:ControlConfiguration id="_IDGenControlConfiguration2"
                                key="REQUESTED_DELIVERY_DATE_VDATU"
                                visibleInAdvancedArea="true"
                                conditionType="{
                                    module: 'sap.ui.comp.config.condition.DateRangeType'
                                }" />
                                <sf:ControlConfiguration id="_IDGenControlConfiguration3"
                                key="OBD_TIMESTAMP_LAST_STATUS_TIME_PLANT_BASED"
                                visibleInAdvancedArea="true"
                                conditionType="{
                                    module: 'sap.ui.comp.config.condition.DateRangeType'
                                }" />
                        </sf:controlConfiguration>
                    </sf:SmartFilterBar>

                    
                </f:DynamicPageHeader>
            </f:header>

            <f:content>
                <st:SmartTable
                    xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
                    width="100%"
                    direction="Column"
                    fitContainer="true"
                    tableType="ResponsiveTable"
                    header="{i18n>tableTitle}"
                    enableAutoBinding="true"
                    id="table0"
                    smartFilterId="bar0"
                    cd:w5g.dt.context="/SHIPPINGSTATUS"
                    entitySet="SHIPPINGSTATUS"
                    placeToolbarInTable="true"
                    useExportToExcel="true"
                    beforeExport="onBeforeExport"
                    useVariantManagement="true"
                    useTablePersonalisation="true"
                    customData:p13nDialogSettings='{filter:{visible:false}}'
                    showRowCount="true"
                    persistencyKey="SmartTableAnalytical_SHIPSTATUS"
                    demandPopin="true"
                    enableAutoColumnWidth="true"
                    class="sapUiResponsiveContentPadding"
                >
                    <t:Table
                        id="gridTable"
                        selectionMode="None"
                        alternateRowColors="true"
                        >
                        <t:columns>
                            <!-- Delivery # -->
                            <t:Column id="_IDGenColumn1" width="8rem" hAlign="End" sortProperty="OBD_NO_DOCNO_C">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"OBD_NO_DOCNO_C","leadingProperty":"OBD_NO_DOCNO_C","columnIndex":0}'/>
                                </t:customData>
                                <Text id="_IDGenText" text="Delivery #" />
                                <t:template>
                                    <Text id="_IDGenText1" text="{OBD_NO_DOCNO_C}" />
                                </t:template>
                            </t:Column>

                            <!-- Requested Delivery Date -->
                            <t:Column id="_IDGenColumn2" width="10rem" hAlign="End" sortProperty="REQUESTED_DELIVERY_DATE_VDATU">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"REQUESTED_DELIVERY_DATE_VDATU","leadingProperty":"REQUESTED_DELIVERY_DATE_VDATU","columnIndex":2}'/>
                                </t:customData>
                                <Text id="_IDGenText2" text="Estimated Delivery Date" />
                                <t:template>
                                    <Text 
                                        id="_IDGenText3" text="{
                                        path: 'REQUESTED_DELIVERY_DATE_VDATU',type: 'sap.ui.model.odata.type.Date'}"/>
                                </t:template>
                            </t:Column>

                            <!-- Document Date -->
                            <t:Column id="_IDGenColumn3" width="10rem" hAlign="End" sortProperty="AUDAT">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"AUDAT","leadingProperty":"AUDAT","columnIndex":1}' />
                                </t:customData>
                                <Text id="_IDGenText4" text="Order Date" />
                                <t:template>
                                <Text 
                                    id="_IDGenText5" text="{
                                    path: 'AUDAT', type: 'sap.ui.model.odata.type.Date'}"
                                />
                                </t:template>
                            </t:Column>

                            <!-- Plant Time (Timestamp) -->
                            <t:Column id="_IDGenColumn4" width="12rem" hAlign="End" sortProperty="OBD_TIMESTAMP_LAST_STATUS_TIME_PLANT_BASED">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"OBD_TIMESTAMP_LAST_STATUS_TIME_PLANT_BASED","leadingProperty":"OBD_TIMESTAMP_LAST_STATUS_TIME_PLANT_BASED","columnIndex":3}' />
                                </t:customData>    
                                <Text id="_IDGenText6" text="Plant Time" />
                                <t:template>
                                    <Text id="_IDGenText7" text="{
                                        path: 'OBD_TIMESTAMP_LAST_STATUS_TIME_PLANT_BASED',
                                        type: 'sap.ui.model.type.DateTime',
                                        formatOptions: { style: 'medium' }
                                    }"/>
                                </t:template>
                            </t:Column>

                            <!-- Warehouse Status -->
                            <t:Column id="_IDGenColumn5" width="12rem" sortProperty="PICK_AND_PACK_STATUS_SALES_SHIPPING_STATUS">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"PICK_AND_PACK_STATUS_SALES_SHIPPING_STATUS","leadingProperty":"PICK_AND_PACK_STATUS_SALES_SHIPPING_STATUS","columnIndex":4}' />
                                </t:customData>
                                <Text id="_IDGenText8" text="Warehouse Status" />
                                <t:template>
                                    <Text id="_IDGenText9" text="{PICK_AND_PACK_STATUS_SALES_SHIPPING_STATUS}" />
                                </t:template>
                            </t:Column>

                            <!-- Quantity Ordered -->
                            <!-- <t:Column id="_IDGenColumn6" width="10rem" hAlign="End" sortProperty="QUANTITY_ORDERED_QTY">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"QUANTITY_ORDERED_QTY","leadingProperty":"QUANTITY_ORDERED_QTY","columnIndex":6}' />
                                </t:customData>
                                <Text id="_IDGenText10" text="Quantity Ordered" />
                                <t:template>
                                    <Text id="_IDGenText11" text="{QUANTITY_ORDERED_QTY}" textAlign="End" />
                                </t:template>
                            </t:Column> -->

                            <!-- Product Description -->
                            <t:Column id="_IDGenColumn7" width="14rem" sortProperty="PRODUCT_DESCRIPTION_MAKTX">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"PRODUCT_DESCRIPTION_MAKTX","leadingProperty":"PRODUCT_DESCRIPTION_MAKTX","columnIndex":6}' />
                                </t:customData>    
                                <Text id="_IDGenText12" text="Product Description" />
                                <t:template>
                                    <Text id="_IDGenText13" text="{PRODUCT_DESCRIPTION_MAKTX}" />
                                </t:template>
                            </t:Column>

                            <!-- Customer PO -->
                            <t:Column id="_IDGenColumn8" width="12rem" sortProperty="CUSTOMER_PO_BSTNK">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"CUSTOMER_PO_BSTNK","leadingProperty":"CUSTOMER_PO_BSTNK","columnIndex":7}' />
                                </t:customData>
                                <Text id="_IDGenText14" text="Customer PO" />
                                <t:template>
                                    <Text id="_IDGenText15" text="{CUSTOMER_PO_BSTNK}" />
                                </t:template>
                            </t:Column>

                            <!-- Sales Order # -->
                            <t:Column id="_IDGenColumn9" width="10rem" sortProperty="SO_NO_REFDOCNO" hAlign="End">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"SO_NO_REFDOCNO","leadingProperty":"SO_NO_REFDOCNO","columnIndex":8}' />
                                </t:customData>
                                <Text id="_IDGenText16" text="Sales Order #" />
                                <t:template>
                                    <Text id="_IDGenText17" text="{SO_NO_REFDOCNO}" />
                                </t:template>
                            </t:Column>

                            <!-- SKU -->
                            <!-- <t:Column id="_IDGenColumn10" width="8rem" hAlign="End" sortProperty="SKU_PRODUCTNO">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"SKU_PRODUCTNO","leadingProperty":"SKU_PRODUCTNO","columnIndex":9}' />
                                </t:customData>
                                <Text id="_IDGenText18" text="SKU" />
                                <t:template>
                                    <Text id="_IDGenText19" text="{SKU_PRODUCTNO}" />
                                </t:template>
                            </t:Column> -->

                            <!-- Manufacturer # -->
                            <t:Column id="_IDGenColumn11" width="10rem" hAlign="End" sortProperty="MANUFACTURER_MFRNR">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"MANUFACTURER_MFRNR","leadingProperty":"MANUFACTURER_MFRNR","columnIndex":10}' />
                                </t:customData>
                                <Text id="_IDGenText20" text="Manufacturer #" />
                                <t:template>
                                    <Text id="_IDGenText21" text="{MANUFACTURER_MFRNR}" />
                                </t:template>
                            </t:Column>

                            <!-- Manufacturer Name -->
                            <t:Column id="_IDGenColumn12" width="14rem" sortProperty="MANUFACTURER_NAME_MC_NAME1">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"MANUFACTURER_NAME_MC_NAME1","leadingProperty":"MANUFACTURER_NAME_MC_NAME1","columnIndex":11}' />
                                </t:customData>
                                <Text id="_IDGenText22" text="Manufacturer Name" />
                                <t:template>
                                    <Text id="_IDGenText23" text="{MANUFACTURER_NAME_MC_NAME1}" />
                                </t:template>
                            </t:Column>

                            <!-- Ship To # -->
                            <t:Column id="_IDGenColumn13" width="10rem" hAlign="End" sortProperty="SHIP_TO_PARTYNO">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"SHIP_TO_PARTYNO","leadingProperty":"SHIP_TO_PARTYNO","columnIndex":12}' />
                                </t:customData>
                                <Text id="_IDGenText24" text="Ship To #" />
                                <t:template>
                                    <Text id="_IDGenText25" text="{SHIP_TO_PARTYNO}" />
                                </t:template>
                            </t:Column>

                            <!-- Ship To Name -->
                            <t:Column id="_IDGenColumn14" width="12rem" sortProperty="SHIP_TO_NAME_PARTNER">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"SHIP_TO_NAME_PARTNER","leadingProperty":"SHIP_TO_NAME_PARTNER","columnIndex":13}' />
                                </t:customData>
                                <Text id="_IDGenText26" text="Ship To Name" />
                                <t:template>
                                    <Text id="_IDGenText27" text="{SHIP_TO_NAME_PARTNER}" />
                                </t:template>
                            </t:Column>

                            <!-- Storage Condition -->
                            <t:Column id="_IDGenColumn15" width="8rem" sortProperty="STORAGE_CONDITIONS_STOKEY1">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"STORAGE_CONDITIONS_STOKEY1","leadingProperty":"STORAGE_CONDITIONS_STOKEY1","columnIndex":14}' />
                                </t:customData>
                                <Text id="_IDGenText28" text="Storage Condition" />
                                <t:template>
                                    <Text id="_IDGenText29" text="{STORAGE_CONDITIONS_STOKEY1}" />
                                </t:template>
                            </t:Column>

                            <!-- Plant -->
                            <t:Column id="_IDGenColumn16" width="12rem" sortProperty="WAREHOUSE_NAME_LNUMT">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"WAREHOUSE_NAME_LNUMT","leadingProperty":"WAREHOUSE_NAME_LNUMT","columnIndex":15}' />
                                </t:customData>
                                <Text id="_IDGenText30" text="Plant" />
                                <t:template>
                                    <Text id="_IDGenText31" text="{WAREHOUSE_NAME_LNUMT}" />
                                </t:template>
                            </t:Column>

                            <!-- Time Zone -->
                            <t:Column id="_IDGenColumn17" width="8rem" sortProperty="WAREHOUSE_TIME_ZONE">
                                <t:customData>
                                <core:CustomData key="p13nData" value='{"columnKey":"WAREHOUSE_TIME_ZONE","leadingProperty":"WAREHOUSE_TIME_ZONE","columnIndex":16}' />
                                </t:customData>
                                <Text id="_IDGenText32" text="Time Zone" />
                                <t:template>
                                    <Text id="_IDGenText33" text="{WAREHOUSE_TIME_ZONE}" />
                                </t:template>
                            </t:Column>

                            <!-- Sales Org -->
                            <t:Column id="_IDGenColumn18" width="6rem" hAlign="End" sortProperty="VKORG">
                                <t:customData>
                                    <core:CustomData key="p13nData" value='{"columnKey":"VKORG","leadingProperty":"VKORG","columnIndex":17}' />
                                </t:customData>
                                <Text id="_IDGenText34" text="Sales Org" />
                                <t:template>
                                    <Text id="_IDGenText35" text="{VKORG}" />
                                </t:template>
                            </t:Column>
                        </t:columns>
                    </t:Table> 
                </st:SmartTable>
            </f:content>
        </f:DynamicPage>
</mvc:View>

